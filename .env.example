# Supabase (Auth & Database)
NEXT_PUBLIC_SUPABASE_URL=your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key

# BulkVS (Voice & SMS)
BULKVS_API_KEY=your-api-key
BULKVS_API_SECRET=your-api-secret
BULKVS_FROM_NUMBER=+1234567890
BULKVS_WEBHOOK_URL=https://your-domain.com/api/webhooks/bulkvs
BULKVS_WEBHOOK_SECRET=your-webhook-secret
# Optional - for shared DID pool
BULKVS_DID_POOL=comma,separated,numbers

# App Config
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Social Media Stream
SOCIAL_STREAM_ENDPOINT=wss://your-social-stream-endpoint

# Rate Limiting (Upstash Redis)
UPSTASH_REDIS_REST_URL=https://your-redis-url.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-redis-token 